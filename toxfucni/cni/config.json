{
    "cniVersion": "1.0.0",
    "name": "toxfu",
    "plugins": [
      {
        "type": "nsexec",
        "plugin": "bridge",
        "bridge": "cni0",
        "ipam": {
          "type": "toxfu-ipam"
        },
        "dns": {
          "nameservers": [ "10.1.0.1" ]
        }
      },
      {
        "type": "tuning",
        "capabilities": {
          "mac": true
        },
        "sysctl": {
          "net.core.somaxconn": "500"
        }
      },
      {
          "type": "portmap",
          "capabilities": {"portMappings": true}
      }
    ]
  }
