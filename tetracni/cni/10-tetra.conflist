{
  "cniVersion": "1.0.0",
  "name": "tetrapod",
  "plugins": [
    {
      "type": "bridge",
      "bridge": "cni0",
      "isDefaultGateway": true,
      "ipam": {
        "type": "tetra-pod-ipam",
        "ipamPlugin": "host-local"
      }
    },
    {
      "type": "hostvrf",
      "vrf": "tetrapod-vrf"
    },
    {
      "type": "route-pods",
      "vrf": "tetrapod-vrf"
    }
  ]
}
